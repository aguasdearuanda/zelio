<section>
  <div class="container">
    <div class="row" align="center">
      <div class="col-sm-12">

        <div class="wrapper-page">
          <div align="center">
            <%= image_tag('aguasdearuanda', height: '120') %>
          </div>
          <div class="col-md-6">
            <%= link_to new_student_session_path do %>
            <div class="m-t-30 card-box">
              <div class="text-center">
                <h2 class="text m-t-0 m-b-30">
                  <span><%= image_tag('students-icon', height: '100') %></span>
                </h2>
                <h6 class="text-uppercase font-bold m-b-0">Aluno</h6>
              </div>
            </div>
            <% end %>
        </div>

        <div class="col-md-6">
          <%= link_to new_admin_session_path do %>
          <div class="m-t-30 card-box">
            <div class="text-center">
              <h2 class="text-uppercase m-t-0 m-b-30">
                <span><%= image_tag('admin-logo', height: '100') %></span>
              </h2>
              <h6 class="text-uppercase font-bold m-b-0">Administrador</h6>
            </div>
          </div>
          <% end %>
          </div>
          <!-- end card-box-->
        </div>
        <!-- end wrapper -->

      </div>
    </div>
  </div>
</section>
